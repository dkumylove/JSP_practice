<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
  <display-name>JSP_WORK</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>INIT_PARAM</param-name>
    <param-value>web.xml에 저장한 초기화 변수</param-value>
  </context-param>
  <context-param>
    <param-name>driver</param-name>
    <param-value>oracle.jdbc.driver.OracleDriver</param-value>
  </context-param>
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:oracle:thin:@localhost:1521:myoracle</param-value>
  </context-param>
  <context-param>
    <param-name>id</param-name>
    <param-value>test</param-value>
  </context-param>
  <context-param>
    <param-name>pw</param-name>
    <param-value>1234</param-value>
  </context-param>
  <resource-ref>
    <description>Oracle Datasource example</description>
    <res-ref-name>jdbc/myoracle</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <error-page>
    <error-code>404</error-code>
    <location>/Exception.jsp</location>
  </error-page>
  <session-config>
    <session-timeout>20</session-timeout>
  </session-config>
  <filter>
    <filter-name>SessionInitializer</filter-name>
    <filter-class>org.apache.catalina.filters.SessionInitializerFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>SessionInitializer</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>BookRegUplosdController</display-name>
    <servlet-name>BookRegUplosdController</servlet-name>
    <servlet-class>com.momo.book.controller.BookRegUplosdController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookRegUplosdController</servlet-name>
    <url-pattern>/BookRegUplosdController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Book_9_RegUplosdController</display-name>
    <servlet-name>Book_9_RegUplosdController</servlet-name>
    <servlet-class>com.momo.book.controller.Book_9_RegUplosdController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Book_9_RegUplosdController</servlet-name>
    <url-pattern>/Book_9_RegUplosdController</url-pattern>
  </servlet-mapping>
</web-app>